<p id="notice"><%= notice %></p>

<h1>Listing Stands</h1>

<table>
  <thead>
    <tr>
      <th>Organisation</th>
      <th>Name</th>
      <th>Address</th>
      <th>Postalcode</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stands.each do |stand| %>
      <tr>
        <td><%= stand.organisation %></td>
        <td><%= stand.name %></td>
        <td><%= stand.address %></td>
        <td><%= stand.postalcode %></td>
        <td><%= stand.longitude %></td>
        <td><%= stand.latitude %></td>
        <td><%= link_to 'Show', stand %></td>
        <td><%= link_to 'Edit', edit_stand_path(stand) %></td>
        <td><%= link_to 'Destroy', stand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stand', new_stand_path %>
